0.2\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-md-6\">\n                <div class=\"google-map\" mbr-map=\"googleMap\">\n                </div>\n            </div>\n            <div class=\"col-md-6\">\n                <h2 mbr-if=\"showTitle\" class=\"pb-3 align-left mbr-fonts-style\" mbr-theme-style=\"display-2\">Contact</h2>\n                <div>\n                    <div class=\"icon-block pb-3\">\n                        <span mbr-if=\"showIcons\" class=\"icon-block__icon\">\n                            <a href=\"mailto:info@goblinfarm.nl\"><span mbr-icon class=\"mbr-iconfont mbri-github\" style=\"color: rgb(118, 118, 118); fill: rgb(118, 118, 118);\"></span></a>\n                        </span>\n                        <h4 mbr-if=\"showSubheading\" class=\"icon-block__title align-left mbr-fonts-style\" mbr-theme-style=\"display-5\"><a href=\"mailto:info@goblinfarm.nl\"><i>Stuur gerust een e-mail!</i></a></h4>\n                    </div>\n                    <div class=\"icon-contacts pb-3\" mbr-if=\"showContacts\">\n                        <h5 mbr-if=\"showSubtitle\" class=\"align-left mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                            Goblin Farm</h5>\n                        <p mbr-if=\"showText\" class=\"mbr-text align-left mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">Boerendijk 4 \n<br>4451RK Heinkenszand&nbsp;</p><p>Tel:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href=\"tel:0031113561504\">0113-561504</a>\n<br>Mail: &nbsp;&nbsp;<a href=\"mailto:info@goblinfarm.nl\">info@goblinfarm.nl</a></p><p></p>\n                    </div>\n                </div>\n                <div mbr-form mbr-if=\"show